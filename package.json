{
  "name": "glom",
  "description": "Fully offline Gaslands (https://gaslands.com/) vehicle squad manager.",
  "repository": {
    "type": "git",
    "url": "git://github.com/milogert/glom.git"
  },
  "private": true,
  "scripts": {
      "init": "yarn elm-github-install",
      "start": "yarn elm-app start",
