  "dependencies": {
    "bootstrap": "^4.1.3",
    "create-elm-app": "^3.0.2",
    "elm": "^0.19.0-bugfix2",
    "elm-coverage": "^0.2.0",
    "elm-format": "^0.8.1",
    "elm-github-install": "^1.6.1",
    "elm-test": "^0.19.0-rev4",
    "gh-pages": "^2.0.1",
    "node-sass": "^4.9.3",
    "sass": "^1.3.2"
  }
}
